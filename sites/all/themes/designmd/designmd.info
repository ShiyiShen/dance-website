;Theme DESIGNMD
name = 'Designmd'
description = 'Clean and modern responsive Drupal theme.'
core = '7.x'
engine = 'phptemplate'
screenshot = 'screenshot.png'


;Stylesheets
stylesheets[all][] = 'bootstrap/css/bootstrap.min.css'
; stylesheets[all][] = 'css/reset.css'
stylesheets[all][] = 'css/style.css'
; stylesheets[all][] = 'css/responsive.css'
stylesheets[all][] = 'css/menu.css'
stylesheets[all][] = 'css/prettyPhoto.css'
stylesheets[all][] = 'owl-carousel/owl.carousel.css'
